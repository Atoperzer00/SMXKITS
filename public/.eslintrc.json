{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "jquery": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "script"
  },
  "globals": {
    "L": "readonly",
    "$": "readonly",
    "jQuery": "readonly",
    "Arma3Map": "writable",
    "MGRS_CRS": "readonly",
    "InitMap": "readonly"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off",
    "no-undef": "error"
  }
}
