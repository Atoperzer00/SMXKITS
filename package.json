{
  "name": "smxkits",
  "version": "1.0.0",
  "description": "SMX KITS Training Management System",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "start:streaming": "docker-compose -f docker-compose.streaming.yml up -d",
    "stop:streaming": "docker-compose -f docker-compose.streaming.yml down",
    "logs:streaming": "docker-compose -f docker-compose.streaming.yml logs -f",
    "setup:streaming": "node setup-streaming.js"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "multer": "^1.4.5-lts.1",
    "redis": "^4.6.10",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.8.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "training",
    "management",
    "education"
  ],
  "author": "SMX KITS",
